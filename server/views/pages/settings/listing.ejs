  <%- include('../../layouts/common/header.ejs') %>
       <div class="container-scroller">
     <%- include('../../layouts/common/navbar.ejs') %>
     <div class="container-fluid page-body-wrapper">
         <%- include("../../layouts/common/main.ejs") %>
          <div class="main-panel">

<!-- Load Summernote CSS -->

<!-- Dashboard Content Starts Here -->
  <div class="content-wrapper">
  <div class="row">
    <div class="col-12 col-md-12 col-lg-12">
      <div class="card">
        <form method="POST" action="/admin/setting/<%= setting.id %>">
          <div class="card-header">
            <h4> Settings</h4>
          </div>
          <div class="card-body">
            <div class="form-group">
              <label for="title">Email</label>
              <input type="email" class="form-control" name="email" value="<%= setting.email %>"  required>
            </div>
             <div class="form-group">
              <label for="phone">Phone</label>
              <input type="tel" class="form-control" name="phone" value="<%= setting.phone %>"  required>
            </div>
             <div class="form-group">
              <label for="title">Facebook link</label>
              <input type="text" class="form-control" name="facebook_link" value="<%= setting.facebook_link %>"  required>
            </div>
             <div class="form-group">
              <label for="title">Instagram link</label>
              <input type="text" class="form-control" name="instagram_link" value="<%= setting.instagram_link %>"  required>
            </div>
             <div class="form-group">
              <label for="title">linkedin link</label>
              <input type="text" class="form-control" name="linkedin_link" value="<%= setting.linkedin_link %>"  required>
            </div>

            <div class="form-group">
              <label for="title">twitter_link</label>
              <input type="text" class="form-control" name="twitter_link" value="<%= setting.twitter_link %>"  required>
            </div>
            <div class="form-group">
              <label for="title">Address</label>
              <input type="text" class="form-control" name="address" value="<%= setting.address %>"  required>
            </div>


          
            <input type="text" name="id" class="form-control" value="<%=setting.id%>" hidden required="">
          </div>
          <div class="card-footer text-right">
            <button type="submit" style='background-color: #157B6C;' class="btn text-white ">Update</button>
          </div>
        </form>
      </div>
    </div>
  </div>
</div>
</div>
</div>
</div>
  <%- include('../../layouts/common/footer.ejs') %>


